{
	"name": "vinsanity",
	"version": "1.0.0",
	"main": "dist/index.js",
	"author": "Jake Klassen",
	"license": "MIT",
	"type": "module",
	"imports": {
		"#app/*": "./dist/*"
	},
	"scripts": {
		"prebuild": "shx rm -rf dist",
		"build": "tsc -p tsconfig.build.json",
		"lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" .eslintrc.cjs jest.config.cjs",
		"watch": "tsc --watch",
		"create-order": "node --import tsx --env-file=.env src/create-order.ts",
		"create-order-with-tax-surplus": "node --import tsx --env-file=.env src/create-order.ts --order-template=orders/order-with-tax-surplus.json",
		"create-order-with-tax-deficit": "node --import tsx --env-file=.env src/create-order.ts --order-template=orders/order-with-tax-deficit.json",
		"start": "tsx src",
		"prod": "node dist"
	},
	"dependencies": {
		"@ngneat/falso": "^7.1.1",
		"axios": "^1.6.2",
		"execa": "^8.0.1",
		"p-map": "^7.0.0",
		"puppeteer": "^21.6.1",
		"zod": "^3.22.4"
	},
	"devDependencies": {
		"@types/eslint": "8.44.9",
		"@types/node": "^20.10.4",
		"@typescript-eslint/eslint-plugin": "6.14.0",
		"@typescript-eslint/parser": "6.14.0",
		"comment-json": "4.2.3",
		"eslint": "^8.55.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-prettier": "5.0.1",
		"expect": "29.7.0",
		"prettier": "3.1.1",
		"shx": "0.3.4",
		"tsm": "2.3.0",
		"tsx": "^4.6.2",
		"typescript": "5.3.3"
	}
}
